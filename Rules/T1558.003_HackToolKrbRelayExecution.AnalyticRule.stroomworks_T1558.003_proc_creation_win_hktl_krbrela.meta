  ],
  "query": "from RULE_VIEW\nwhere ((((EventData.Image = '*\\\\\\\\KrbRelay.exe') or (EventData.OriginalFileName = '*KrbRelay.exe*')) or ((EventData.CommandLine = '* -spn *' or EventData.CommandLine = '* -clsid *' or EventData.CommandLine = '* -rbcd *'))) or ((EventData.CommandLine = '*shadowcred*' or EventData.CommandLine = '*clsid*' or EventData.CommandLine = '*spn*'))) or ((EventData.CommandLine = '*spn *' or EventData.CommandLine = '*session *' or EventData.CommandLine = '*clsid *'))\nselect *",
